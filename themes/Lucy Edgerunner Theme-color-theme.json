  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#00f3ff", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#ff0055", "fontStyle": "bold" }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#00f3ff" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#dbebf7", "fontStyle": "italic" }
    },
    {
      "scope": "variable",
      "settings": { "foreground": "#ffffff" }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "scope": "entity.name.type",
      "settings": { "foreground": "#7000ff" }
    },
    {
      "scope": ["markup.bold", "markup.heading"],
      "settings": { "foreground": "#ff0055", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": "markup.inserted",
      "settings": { "foreground": "#00f3ff" }
    },
    {
      "scope": "markup.deleted",
      "settings": { "foreground": "#ff0055" }
    }
  ],
  "editor.semanticHighlighting.enabled": true,
    "editor.tokenColorCustomizations": {
      "textMateRules": [
        {
          "name": "Lucy: Keywords (public, static, int)",
          "scope": [
            "keyword",
            "storage.type",
            "storage.modifier",
            "keyword.control"
          ],
            "settings": {
            "foreground": "#ff0055",
            "fontStyle": "bold"
          }
        },
        {
          "name": "Lucy: Nombre de la función (suma)",
          "scope": [
            "entity.name.function",
            "variable.function",
            "support.function"
          ],
          "settings": {
            "foreground": "#994dff",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Lucy: Variables y parámetros",
          "scope": [
            "variable",
            "variable.other",
            "variable.parameter"
          ],
          "settings": {
            "foreground": "#dbebf7"
          }
        },
        {
          "name": "Lucy: Números y constantes",
          "scope": ["constant.numeric", "constant.language"],
          "settings": {
            "foreground": "#ffcc00"
          }
        },
        {
          "name": "Lucy: Operadores y puntuación",
          "scope": [
            "keyword.operator",
            "punctuation",
            "meta.brace",
            "punctuation.definition.parameters"
          ],
          "settings": {
            "foreground": "#00f3ff"
          }
        },
        {
          "name": "Forzar color comentarios",
          "scope": "comment",
          "settings": {
            "foreground": "#00f3ff",
            "fontStyle": "italic"
          }
        }
      ]
    }
}